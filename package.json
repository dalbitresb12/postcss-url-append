{
  "name": "postcss-url-append",
  "version": "0.1.0",
  "description": "PostCSS plugin to append a raw block of CSS from a URL.",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "append",
    "url"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint . && tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:full": "jest --coverage"
  },
  "author": {
    "name": "Diego Albitres",
    "email": "diego.albitres@gmail.com",
    "url": "https://dalbitresb.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dalbitresb12/postcss-url-append.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=12.0.0"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.10",
    "@types/jest": "^26.0.23",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^15.0.3",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "clean-publish": "^2.1.0",
    "eslint": "^7.21.0",
    "eslint-plugin-jest": "^24.2.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.15",
    "simple-git-hooks": "^2.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "is-url-superb": "^6.0.0",
    "mime-types": "^2.1.30",
    "node-fetch": "^2.6.1",
    "prepend-http": "^3.0.1"
  }
